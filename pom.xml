        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.7.10</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <groupId>com.zbank</groupId>
    <artifactId>David-SpringCloud</artifactId>
    <version>1.0</version>

    <properties>
