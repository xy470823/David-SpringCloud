        <thread-local.version>2.14.2</thread-local.version>
        <commons-lang3.version>3.12.0</commons-lang3.version>
        <consul-api.version>1.4.5</consul-api.version>
        <lombok.version>1.18.26</lombok.version>
        <netty-all.version>5.0.0.Alpha2</netty-all.version>
        <common-netty.version>1.0</common-netty.version>
